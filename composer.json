{
    "name": "medieval-rus/birchbark-literacy",
    "description": "«Birchbark Literacy from Medieval Rus» database",
    "type": "project",
    "keywords": ["rus" ,"medieval rus'", "birchbark", "linguistics", "science"],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Anton Dyshkant",
            "email": "vyshkant@gmail.com",
            "homepage": "https://vyshkant.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "knplabs/knp-menu-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.91",
        "sonata-project/doctrine-orm-admin-bundle": "^3.30",
        "sonata-project/media-bundle": "^3.30",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vyfony/bibliography-bundle": "^0.1.1"
    },
    "require-dev": {
        "enlightn/security-checker": "^1.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.7",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp": "^1.0",
        "povils/phpmnd": "^2.3",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "*",
        "symfony/stopwatch": "^4.4",
        "symfony/test-pack": "*",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
